{
  "presets": [
    "flow",
    [
      "env",
      {
        "targets": {
          "node": "6.14.0"
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-flow-strip-types",
    "add-module-exports",
    "transform-runtime",
    "transform-imports",
    "transform-inline-environment-variables"
  ]
}